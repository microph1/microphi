{
  "name": "microphi",
  "version": "0.0.0",
  "workspaces": [
    "projects/*"
  ],
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "DEBUG=microphi:* ng test",
    "test:ci": "ng test --watch=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "version": "lerna version",
    "predeploy:dcsoftware": "ng build dcsoftware --configuration production",
    "deploy:dcsoftware": "cd projects/dcsoftware && yarn sls client deploy --no-confirm",
    "deploy:store": "ng deploy store --configuration=production",
    "predeploy:microphi": "ng build microphi --configuration production",
    "deploy": "lerna run deploy --stream --scope",
    "docs": "lerna run docs --stream",
    "postdocs": "node --experimental-modules scripts/post-doc.mjs",
    "postinstall": "ngcc"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.16",
    "@angular/cdk": "~12.2.13",
    "@angular/common": "~12.2.16",
    "@angular/compiler": "~12.2.16",
    "@angular/core": "~12.2.16",
    "@angular/flex-layout": "^12.0.0-beta.35",
    "@angular/forms": "~12.2.16",
    "@angular/material": "^12.2.13",
    "@angular/platform-browser": "~12.2.16",
    "@angular/platform-browser-dynamic": "~12.2.16",
    "@angular/platform-server": "~12.2.16",
    "@angular/router": "~12.2.16",
    "@microgamma/loggator": "^1.16.0",
    "@unbounded/unbounded": "^0.9.9",
    "ngx-deploy-npm": "^1.2.2",
    "rxjs": "~6.5.4",
    "tslib": "^2.0.0",
    "utility-types": "^3.10.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.17",
    "@angular/cli": "~12.2.17",
    "@angular/compiler-cli": "~12.2.16",
    "@angular/language-service": "~12.2.16",
    "@compodoc/compodoc": "^1.1.11",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.17",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-mocha-reporter": "^2.2.5",
    "lerna": "^3.20.2",
    "ng-packagr": "^12.2.7",
    "protractor": "~7.0.0",
    "serverless": "^1.62.0",
    "serverless-finch": "^2.5.2",
    "ts-node": "^8.6.2",
    "tslint": "~6.1.0",
    "typedoc": "^0.16.9",
    "typescript": "~4.3.5"
  }
}
