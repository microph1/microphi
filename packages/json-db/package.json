{
  "name": "@microphi/json-db",
  "version": "2.7.0",
  "description": "Small, fast and simple file based json-db",
  "main": "lib/cjs/index.js",
  "typings": "lib/esm/index.d.ts",
  "module": "lib/esm/index.js",
  "exports": {
    ".": {
      "import": "lib/esm/index.js",
      "require": "lib/cjs/index.js"
    }
  },
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib"
  ],
  "license": "MIT",
  "keywords": [
    "json-db"
  ],
  "author": {
    "email": "cavaliere.davide@gmail.com",
    "name": "Davide Cavaliere",
    "url": "www.dcsoftware.io"
  },
  "homepage": "https://github.com/microph1/microphi",
  "scripts": {
    "build": "tsc -b tsconfig.build.json && tsc -b tsconfig.cjs.json",
    "build:ci": "yarn build",
    "test:ci": "jest --coverage --ci",
    "prepack": "yarn build",
    "lint": "eslint ./src"
  },
  "dependencies": {
    "@microphi/debug": "^2.6.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.23.3",
    "@datakitchen/rxjs-marbles": "^17.0.0",
    "@types/node": "^20.11.30",
    "minisearch": "^6.3.0",
    "utility-types": "^3.10.0"
  },
  "peerDependencies": {
    "@microphi/list": "^2.3.0",
    "minisearch": "^6.3.0",
    "rxjs": "~7.8.0"
  }
}
