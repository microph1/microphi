{
  "name": "microphi",
  "version": "0.0.0",
  "workspaces": [
    "projects/*"
  ],
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "DEBUG=microphi:* ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "version": "lerna version",
    "predeploy:dcsoftware": "ng build dcsoftware --prod",
