{
  "name": "microphi",
  "version": "0.0.0",
  "workspaces": [
    "projects/*",
    "projects/flux/*",
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "jest",
    "test:ci": "jest --ci --coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "release": "lerna version --conventional-commits",
    "predeploy:dcsoftware": "ng build dcsoftware --configuration production",
    "deploy:dcsoftware": "cd projects/dcsoftware && yarn sls client deploy --no-confirm",
    "deploy:store": "ng deploy store --configuration=production",
    "predeploy:microphi": "ng build microphi --configuration production",
    "deploy": "lerna run deploy --stream --scope",
    "docs": "lerna run docs --stream",
    "postdocs": "node --experimental-modules scripts/post-doc.mjs"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.0",
    "@angular/cdk": "16",
    "@angular/common": "^16.2.0",
    "@angular/compiler": "^16.2.0",
    "@angular/core": "^16.2.0",
    "@angular/forms": "^16.2.0",
    "@angular/material": "16",
    "@angular/platform-browser": "^16.2.0",
    "@angular/platform-browser-dynamic": "^16.2.0",
    "@angular/router": "^16.2.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "@microgamma/loggator": "^2.1.0",
    "ngx-deploy-npm": "^4.1.0",
    "ts-loader": "^9.3.1",
    "utility-types": "^3.10.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.11",
    "@angular-eslint/builder": "16.3.1",
    "@angular-eslint/eslint-plugin": "16.3.1",
    "@angular-eslint/eslint-plugin-template": "16.3.1",
    "@angular-eslint/schematics": "16.3.1",
    "@angular-eslint/template-parser": "16.3.1",
    "@angular/cli": "^16.2.11",
    "@angular/compiler-cli": "^16.2.0",
    "@types/jest": "^29.4.4",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "eslint": "^8.51.0",
    "jest": "^29.4.3",
    "jest-environment-jsdom": "^29.4.3",
    "jest-junit": "^16.0.0",
    "jest-preset-angular": "^13.1.1",
    "lerna": "^8.0.2",
    "ng-packagr": "^16.2.0",
    "ts-jest": "^29.1.0",
    "typescript": "~5.1.3",
    "utility-types": "^3.11.0",
    "@faker-js/faker": "^7.3.0",
    "@types/node": "^18.0.1",
    "debug": "^4.3.4",
    "protractor": "~7.0.0",
    "serverless": "^3.19.0",
    "serverless-finch": "^4.0.0",
    "ts-node": "^10.8.1"
  }
}
