{
  "name": "@microphi/debug",
  "version": "2.5.1",
  "description": "A very simple wrapper to console.log",
  "main": "lib/cjs/index.js",
  "typings": "lib/esm/index.d.ts",
  "module": "lib/esm/index.js",
  "exports": {
    ".": {
      "import": "lib/esm/index.js",
      "require": "lib/cjs/index.js"
    }
  },
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib"
  ],
  "licence": "MIT",
  "private": false,
  "keywords": [
    "log",
    "debug",
    "console"
  ],
  "author": {
    "email": "cavaliere.davide@gmail.com",
    "name": "Davide Cavaliere",
    "url": "www.dcsoftware.io"
  },
  "homepage": "https://github.com/microph1/microphi",
  "dependencies": {
    "tslib": "^2.5.0"
  },
  "scripts": {
    "build": "tsc -b tsconfig.build.json && tsc -b tsconfig.cjs.json",
    "build:ci": "yarn build",
    "test:ci": "jest --coverage --ci",
    "prepack": "yarn build",
    "lint": "eslint ./src"
  }
}
