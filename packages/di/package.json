{
  "name": "@microphi/di",
  "version": "2.10.1",
  "description": "Handcrafted DI container",
  "exports": {
    ".": {
      "import": "./lib/esm/index.js",
      "types": "./lib/esm/index.d.ts",
      "require": "./lib/cjs/index.js"
    },
    "./testing": {
      "import": "./lib/esm/lib/testing/index.js",
      "types": "./lib/esm/lib/testing/index.d.ts",
      "require": "./lib/cjs/lib/testing/index.js"
    }
  },
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib"
  ],
  "license": "MIT",
  "keywords": [
    "typescript",
    "decorator",
    "dependency injection",
    "di container"
  ],
  "author": {
    "email": "cavaliere.davide@gmail.com",
    "name": "Davide Cavaliere",
    "url": "www.microphi.io"
  },
  "homepage": "https://github.com/microph1/microphi",
  "scripts": {
    "build": "tsc -b tsconfig.build.json && tsc -b tsconfig.cjs.json",
    "build:ci": "npm run build",
    "test:ci": "jest --coverage --ci",
    "lint": "eslint ./src"
  },
  "peerDependencies": {
    "reflect-metadata": "^0.2.2"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.23.3"
  },
  "sideEffects": false
}
