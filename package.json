{
  "name": "microphi",
  "version": "0.0.0",
  "workspaces": [
    "projects/*"
  ],
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "jest",
    "test:ci": "jest --ci --coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "version": "lerna version",
    "predeploy:dcsoftware": "ng build dcsoftware --configuration production",
    "deploy:dcsoftware": "cd projects/dcsoftware && yarn sls client deploy --no-confirm",
    "deploy:store": "ng deploy store --configuration=production",
    "predeploy:microphi": "ng build microphi --configuration production",
    "deploy": "lerna run deploy --stream --scope",
    "docs": "lerna run docs --stream",
    "postdocs": "node --experimental-modules scripts/post-doc.mjs",
    "postinstall": "ngcc"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.0.0",
    "@angular/cdk": "^14.0.0",
    "@angular/common": "^14.0.0",
    "@angular/compiler": "^14.0.0",
    "@angular/core": "^14.0.0",
    "@angular/flex-layout": "^13.0.0-beta.38",
    "@angular/forms": "^14.0.0",
    "@angular/material": "^14.0.0",
    "@angular/platform-browser": "^14.0.0",
    "@angular/platform-browser-dynamic": "^14.0.0",
    "@angular/platform-server": "^14.0.0",
    "@angular/router": "^14.0.0",
    "@microgamma/loggator": "^2.0.0",
    "ngx-deploy-npm": "^3.1.9",
    "rxjs": "~7.5.5",
    "tslib": "^2.0.0",
    "utility-types": "^3.10.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.0.0",
    "@angular/cli": "^14.0.0",
    "@angular/compiler-cli": "^14.0.0",
    "@angular/language-service": "^14.0.0",
    "@types/jest": "^28.1.1",
    "@types/node": "^17.0.41",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "debug": "^4.3.4",
    "eslint": "^8.17.0",
    "jest": "^28.1.1",
    "jest-preset-angular": "^12.1.0",
    "lerna": "^5.1.0",
    "ng-packagr": "^14.0.0",
    "protractor": "~7.0.0",
    "serverless": "^3.19.0",
    "serverless-finch": "^4.0.0",
    "ts-jest": "^28.0.4",
    "ts-node": "^10.8.1",
    "typescript": "~4.6.3"
  }
